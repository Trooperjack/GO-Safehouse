<?xml version="1.0" ?>
<dynamic_object>
	<diesel materials="units/pd2_mod_tj_csgo_deSafeHouse/csgo/de_safehouse/de_safehouse_mat" orientation_object="rp_safehouse_door" />
	<sequence_manager file="units/pd2_mod_tj_csgo_deSafeHouse/csgo/de_safehouse/doors/door_interior_112_01" />

	<animations>
		<animation_group name="anim_door_open_1" enabled="false">
			<object name="anim_door_open_1" />
		</animation_group>
		<animation_group name="anim_door_open_2" enabled="false">
			<object name="anim_door_open_2" />
		</animation_group>
	</animations>

	<bodies>
		<body name="body_door" enabled="true" template="animated">
			<object name="c_box_001" collision_type="box" padding="-2.5"/>
		</body>
		<body name="body_hitbox_door_handle" enabled="true" template="static">
			<object name="rp_safehouse_door" />
			<object name="c_hitbox_door_handle" collision_type="box" padding="-2.5" />
		</body>
	</bodies>

	<decal_surfaces default_material="wood" static="false">
		<decal_mesh name="dm_wood_001" 		enabled="true" 	material="wood" />
		<decal_mesh name="dm_wood_002" 		enabled="false" 	material="wood" />
		<decal_mesh name="dm_metal" 		enabled="true" 	material="steel_no_decal" />
	</decal_surfaces>

	<graphics>
		<graphic_group name="icon" 	enabled="true" culling_object="g_icon">
			<lod_object name="icon_lod">
					<object name="g_icon" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_icon_lod1" 	enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>
		<graphic_group name="doors" enabled="true">
			<object name="g_door_001" 				enabled="true" 	shadow_caster="true"/>
			<object name="g_door_002" 				enabled="false" shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>


