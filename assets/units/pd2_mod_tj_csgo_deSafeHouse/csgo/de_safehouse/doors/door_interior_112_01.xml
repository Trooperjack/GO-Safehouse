<sequence_manager>
	<unit>
		
		<variables>
			<var_open_in_state value="1"/>
			<var_open_out_state value="0"/>
		</variables>
		
		<filter name="'filter_is_open_in'">
			<check value="vars.var_open_in_state == 1"/>
			<check value="vars.var_open_out_state == 0"/>
		</filter>
		
		<filter name="'filter_is_open_out'">
			<check value="vars.var_open_in_state == 0"/>
			<check value="vars.var_open_out_state == 1"/>
		</filter>
		
		<sequence editable_state="true" name="'set_door_open_in'" triggable="true">
			<set_variables var_open_in_state="1"/>
			<set_variables var_open_out_state="0"/>
		</sequence>
		
		<sequence editable_state="true" name="'set_door_open_out'" triggable="true">
			<set_variables var_open_in_state="0"/>
			<set_variables var_open_out_state="1"/>
		</sequence>
		
		
		
		<sequence editable_state="true" name="'state_door_open_in'" triggable="true">
			<set_variables var_open_in_state="1"/>
			<set_variables var_open_out_state="0"/>
			<interaction enabled="false"/>
			<body enabled="false" name="'body_hitbox_door_handle'"/>
			<animation_group enabled="true" from="35/30" name="'anim_door_open_1'" speed="0" to="35/30"/>
			<run_sequence name="'done_opened'"/>
			<graphic_group name="'icon'" visibility="false"/>
		</sequence>
		
		<sequence editable_state="true" name="'state_door_open_out'" triggable="true">
			<set_variables var_open_in_state="0"/>
			<set_variables var_open_out_state="1"/>
			<interaction enabled="false"/>
			<body enabled="false" name="'body_hitbox_door_handle'"/>
			<animation_group enabled="true" from="75/30" name="'anim_door_open_1'" speed="0" to="75/30"/>
			<run_sequence name="'done_opened'"/>
			<graphic_group name="'icon'" visibility="false"/>
		</sequence>
		
		<sequence editable_state="true" name="'state_door_close'" triggable="true">
			<interaction enabled="true"/>
			<body enabled="true" name="'body_hitbox_door_handle'"/>
			<animation_group enabled="true" from="0/30" name="'anim_door_open_1'" speed="0" to="0/30"/>
			<graphic_group name="'icon'" visibility="true"/>
		</sequence>
		
		
		
		<sequence editable_state="true" name="'state_vis_hide'" triggable="true">
			<body enabled="false" name="'body_hitbox_door_handle'"/>
			<body enabled="false" name="'body_door'"/>
			<graphic_group name="'doors'" visibility="false"/>
		</sequence>
		
		<sequence editable_state="true" name="'state_vis_show'" triggable="true">
			<body enabled="true" name="'body_hitbox_door_handle'"/>
			<body enabled="true" name="'body_door'"/>
			<graphic_group name="'doors'" visibility="true"/>
			<graphic_group name="'icon'" visibility="true"/>
		</sequence>
		
		
		
		<sequence editable_state="true" name="'anim_open_door_in'" triggable="true">
			<body enabled="false" name="'body_hitbox_door_handle'"/>
			<animation_group enabled="true" from="0/30" name="'anim_door_open_1'" speed="2" to="35/30"/>
			<sound action="'play'" event="'generic_door_wood_open'" object="'anim_door_open_1'"/>
			<graphic_group name="'icon'" visibility="false"/>
			<run_sequence name="'done_opened'"/>
		</sequence>
		
		<sequence editable_state="true" name="'anim_open_door_out'" triggable="true">
			<body enabled="false" name="'body_hitbox_door_handle'"/>
			<animation_group enabled="true" from="40/30" name="'anim_door_open_1'" speed="2" to="75/30"/>
			<sound action="'play'" event="'generic_door_wood_open'" object="'anim_door_open_1'"/>
			<graphic_group name="'icon'" visibility="false"/>
			<run_sequence name="'done_opened'"/>
		</sequence>
		
		
		
		<sequence editable_state="true" name="'explode_effect_in'" triggable="true">
			<effect name="'effects/payday2/particles/wood/wood_door_shotgun_dmg_in'" parent="object( 'interaction_point' )" position="v()"/>
		</sequence>
		
		<sequence editable_state="true" name="'explode_effect_out'" triggable="true">
			<effect name="'effects/payday2/particles/wood/wood_door_shotgun_dmg_out'" parent="object( 'interaction_point' )" position="v()"/>
		</sequence>
		
		
		
		<body name="'body_hitbox_door_handle'">
			<endurance bullet="4">
				<run_sequence name="'open_door_exploded'"/>
			</endurance>
			<endurance lock="12">
				<run_sequence name="'open_door_exploded'"/>
			</endurance>
			<endurance explosion="1">
				<run_sequence name="'open_door_exploded'"/>
			</endurance>
		</body>
		
		
		<sequence editable_state="false" name="'interact'" triggable="true">
			<run_sequence name="'open_door'"/>
		</sequence>
				
		<sequence editable_state="true" name="'open_door'" triggable="true">
			<interaction enabled="false"/>
			<run_sequence filter="'filter_is_open_in'" name="'anim_open_door_in'"/>
			<run_sequence filter="'filter_is_open_out'" name="'anim_open_door_out'"/>
			<body enabled="false" name="'body_hitbox_door_handle'"/>
			<run_sequence name="'done_opened'"/>
		</sequence>
		
		<sequence editable_state="true" name="'open_door_exploded'" triggable="true">
			<interaction enabled="false"/>
			<object enabled="false" name="'g_door_001'"/>
			<object enabled="true" name="'g_door_002'"/>
			<decal_mesh enabled="false" name="'dm_wood_001'"/>
			<decal_mesh enabled="true" name="'dm_wood_002'"/>
			<decal_mesh enabled="false" name="'dm_metal'"/>
			<run_sequence filter="'filter_is_open_in'" name="'anim_open_door_in'"/>
			<run_sequence filter="'filter_is_open_out'" name="'anim_open_door_out'"/>
			<run_sequence filter="'filter_is_open_in'" name="'explode_effect_in'"/>
			<run_sequence filter="'filter_is_open_out'" name="'explode_effect_out'"/>
			<body enabled="false" name="'body_hitbox_door_handle'"/>
			<run_sequence name="'done_opened'"/>
			<run_sequence name="'done_exploded'"/>
		</sequence>
		
		
		<sequence editable_state="false" name="'done_exploded'" triggable="false"/>
		<sequence editable_state="false" name="'done_opened'" triggable="false"/>
		<sequence editable_state="false" name="'done_swat_breach'" triggable="false"/>
		
	</unit>
</sequence_manager>
